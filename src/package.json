{
  "name": "app-crud",
  "version": "1.0.0",
  "description": "",
  "main": "build/app/index.js",
  "scripts": {
    "build": "npx -y tsc",
    "db:salt:generate": "node ./app/utils/test",
    "db:drop": "npx sequelize-cli db:drop",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:restart": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "predev": "npm run build && NODE_ENV=development npm run db:restart",
    "prestart": "npm run build",
    "pretest": "NODE_ENV=test npm run db:restart",
    "start": "node .",
    "test": "node ./node_modules/mocha/bin/mocha",
    "mocha": "mocha",
    "dev": "NODE_ENV=development ts-node-dev ./app"
  },
  "author": "Arturo Vaine",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.19.0",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ts": "^0.0.21",
    "swagger-ui-express": "^4.5.0"
  },
  "devDependencies": {
    "@tsconfig/node17": "^1.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.34",
    "@types/sinon": "^10.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "chai": "^4.3.6",
    "eslint": "^8.15.0",
    "install": "^0.13.0",
    "jest": "^28.1.3",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "npm": "^8.17.0",
    "sinon": "13.0.1",
    "swagger-ts-generator": "^2.0.6",
    "ts-node-dev": "^1.1.8"
  }
}
